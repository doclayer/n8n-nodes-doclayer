  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://doclayer.ai",
  "author": {
    "name": "Doclayer",
    "email": "info@doclayer.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doclayer/n8n-nodes-doclayer.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "dist/nodes/Doclayer/Doclayer.node.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/DoclayerApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Doclayer/Doclayer.node.js",
      "dist/nodes/Doclayer/DoclayerTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
